<script>
        $(document).ready(function(){
        $('.progress .progress-bar').progressbar({
            display_text: 'fill'
        });
        $(window).scroll(function(){

            if ($(this).scrollTop() > 600) {
                $('.scrollup').fadeIn();

            } else {
                $('.scrollup').fadeOut();
            }
        });
        $('.scrollup').click(function(){
            $("html, body").animate({
                scrollTop: 0
            }, 600);
            return false;
        });
        $('#cropButton').click(function(){
            var iframe = $("#cropiFrame");
            iframe.attr("src", iframe.data("src"));
        });
    });
