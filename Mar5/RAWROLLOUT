  </section>

  <script>
    document.addEventListener('DOMContentLoaded', function() {
      // Simple counter animation for statistics
      function animateCounter(elementId, targetValue) {
        const element = document.getElementById(elementId);
        if (!element) return;
        
        let prefix = '';
        let suffix = '';
        
        if (elementId === 'commissionValue') {
          prefix = '$';
          suffix = 'K+';
        } else if (elementId === 'approvalRate') {
          suffix = '%';
        } else if (elementId === 'affiliateCount') {
          suffix = '+';
        }
        
        const duration = 2000; // ms
        const frameRate = 60;
        const frames = duration / 1000 * frameRate;
        
        let value = 0;
        const increment = targetValue / frames;
        
        const counter = setInterval(() => {
          value += increment;
          if (value >= targetValue) {
            value = targetValue;
            clearInterval(counter);
          }
          
          // Format based on which counter we're updating
          let displayValue;
          if (elementId === 'approvalRate') {
            displayValue = Math.floor(value);
          } else {
            displayValue = Math.floor(value);
          }
          
          element.textContent = prefix + displayValue + suffix;
        }, duration / frames);
      }
      
      // Function to check if element is in viewport
      function isInViewport(element) {
        const rect = element.getBoundingClientRect();
        return (
          rect.top >= 0 &&
          rect.left >= 0 &&
          rect.bottom <= (window.innerHeight || document.documentElement.clientHeight) &&
          rect.right <= (window.innerWidth || document.documentElement.clientWidth)
        );
      }
      
      // Animate counters when they come into view
      const counterElements = ['affiliateCount', 'commissionValue', 'approvalRate'];
      const targetValues = [500, 100, 95];
      
      function checkCounters() {
        counterElements.forEach((id, index) => {
          const element = document.getElementById(id);
          if (element && isInViewport(element) && !element.classList.contains('animated')) {
            element.classList.add('animated');
            animateCounter(id, targetValues[index]);
          }
        });
      }
      
      // Initial check
      checkCounters();
      
      // Check when scrolling
      window.addEventListener('scroll', checkCounters);
      
      // Handle accessibility preference for reduced motion
      const prefersReducedMotion = window.matchMedia('(prefers-reduced-motion: reduce)');
      if (prefersReducedMotion.matches) {
        // Immediately set values without animation for users who prefer reduced motion
        counterElements.forEach((id, index) => {
          const element = document.getElementById(id);
          if (element) {
            let prefix = id === 'commissionValue' ? '$' : '';
            let suffix = id === 'commissionValue' ? 'K+' : (id === 'approvalRate' ? '%' : '+');
            element.textContent = prefix + targetValues[index] + suffix;
          }
        });
      }
    });
  </script>
</div>
</section>
