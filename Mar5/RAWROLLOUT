    <!-- Back to top button -->
    <button id="backToTop" aria-label="Back to top" class="fixed bottom-8 right-8 bg-[#bb0000] hover:bg-[#ffa500] text-white w-10 h-10 rounded-full flex items-center justify-center shadow-lg transition-all duration-300 opacity-0 invisible">
