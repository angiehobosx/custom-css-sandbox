  <script>
    document.addEventListener('DOMContentLoaded', function() {
      // Tab functionality
      const tabs = document.querySelectorAll('.faq-tab');
      const panels = document.querySelectorAll('.faq-panel');
      
      tabs.forEach(tab => {
        tab.addEventListener('click', () => {
          // Deactivate all tabs
          tabs.forEach(t => {
            t.classList.remove('active');
            t.classList.remove('bg-[#010839]');
            t.classList.add('bg-[#e5e5e5]');
            t.classList.remove('text-white');
            t.classList.add('text-[#3a3a3a]');
            t.setAttribute('aria-selected', 'false');
          });
          
          // Activate clicked tab
          tab.classList.add('active');
          tab.classList.add('bg-[#010839]');
          tab.classList.remove('bg-[#e5e5e5]');
          tab.classList.add('text-white');
          tab.classList.remove('text-[#3a3a3a]');
          tab.setAttribute('aria-selected', 'true');
          
          // Hide all panels
          panels.forEach(panel => {
            panel.classList.add('hidden');
            panel.classList.remove('active');
          });
          
          // Show corresponding panel
          const panelId = tab.getAttribute('aria-controls');
          const activePanel = document.getElementById(panelId);
          activePanel.classList.remove('hidden');
          activePanel.classList.add('active');
        });
      });
      
      // Accordion functionality
      const questionButtons = document.querySelectorAll('.faq-question');
      
      questionButtons.forEach(button => {
        button.addEventListener('click', () => {
          const answer = button.nextElementSibling;
          const icon = button.querySelector('.faq-icon i');
          const isExpanded = button.getAttribute('aria-expanded') === 'true';
          
          // Toggle current item
          answer.classList.toggle('hidden');
          button.setAttribute('aria-expanded', !isExpanded);
          
          if (isExpanded) {
            icon.classList.remove('fa-minus');
            icon.classList.add('fa-plus');
            button.querySelector('.faq-icon').classList.remove('transform', 'rotate-45');
          } else {
            icon.classList.remove('fa-plus');
            icon.classList.add('fa-minus');
            button.querySelector('.faq-icon').classList.add('transform', 'rotate-45');
          }
          
          // Close other items in the same panel
          const panel = button.closest('.faq-panel');
          const otherButtons = panel.querySelectorAll('.faq-question');
          
          otherButtons.forEach(otherButton => {
            if (otherButton !== button) {
              const otherAnswer = otherButton.nextElementSibling;
              const otherIcon = otherButton.querySelector('.faq-icon i');
              
              otherAnswer.classList.add('hidden');
              otherButton.setAttribute('aria-expanded', 'false');
              otherIcon.classList.remove('fa-minus');
              otherIcon.classList.add('fa-plus');
              otherButton.querySelector('.faq-icon').classList.remove('transform', 'rotate-45');
            }
          });
        });
      });
      
      // Open first question in active panel by default
      const activePanel = document.querySelector('.faq-panel.active');
      if (activePanel) {
        const firstQuestion = activePanel.querySelector('.faq-question');
        if (firstQuestion) {
          firstQuestion.click();
        }
      }
    });
  </script>
</div>
</section>
